<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.user.dao.UserMapper" >
    <resultMap id="UserResultMap" type="com.example.user.po.User" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="login_name" property="login" jdbcType="VARCHAR" />
        <result column="password" property="password" jdbcType="VARCHAR" />
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
        <result column="create_by" property="createBy" jdbcType="INTEGER" />
    </resultMap>

    <insert id="insert" parameterType="com.example.user.po.User" useGeneratedKeys="true" keyProperty="id">
        insert into sys_user (
          org_id,
          name,
          login_name,
          password,
          job_number,
          user_type,
          photo,
          email,
          mobile,
          phone,
          create_time,
          create_by,
          del_flag,
          remarks)
        values (
          #{orgID},
          #{name},
          #{login},
          #{password},
          #{jobNumber},
          #{userType},
          #{photo},
          #{email},
          #{mobile},
          #{phone},
          #{createTime},
          #{createBy},
          #{deleteFlag},
          #{remarks})
    </insert>
</mapper>